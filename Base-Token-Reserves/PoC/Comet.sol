pragma solidity ^0.8.0;

contract Comet {
    int _reserveBalance = 110;
    
    function getReserves() public view returns (int) {
        return _reserveBalance;
    }
    
    function setReserveData(int _resBal) public{
        _reserveBalance = _resBal;
    }
}